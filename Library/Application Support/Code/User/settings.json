{
    "editor.fontFamily": "'IBM 3270', 'Hack', Monaco, monospace",
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "editor.lineNumbers": "relative",
    "editor.tabCompletion": "on",
    "editor.cursorBlinking": "phase",
    "editor.acceptSuggestionOnEnter": "off",
    "explorer.confirmDragAndDrop": false,
    "editor.formatOnSave": true,
    "prettier.eslintIntegration": true,
    "window.zoomLevel": 3,
    "workbench.iconTheme": "vs-minimal",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.enablePreview": false,
    "gitlens.codeLens.enabled": false,
    "explorer.openEditors.visible": 0,
    "liveshare.featureSet": "insiders",
    "editor.fontLigatures": true,
    "debug.console.fontFamily": "'IBM 3270'",
    "markdown.preview.fontFamily": "'IBM 3270', -apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'Ubuntu', 'Droid Sans', sans-serif",
    "terminal.integrated.fontFamily": "'IBM 3270'",
    "vim.easymotionMarkerFontFamily": "'IBM 3270'",
    "zenMode.restore": true,
    "zenMode.hideLineNumbers": false,
    "zenMode.fullScreen": false,
    "workbench.colorTheme": "Activate IZZET protocol",
    "zenMode.hideStatusBar": false,
    "workbench.startupEditor": "none"
}
